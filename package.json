{
  "name": "firebase-graphql",
  "private": true,
  "engines": {
    "node": "10"
  },
  "main": "dist/functions/index.js",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml",
    "build:functions-deps": "mkdir -p dist/functions && cp src/functions/package.template.json dist/functions/package.json",
    "build:functions": "npm run build:functions-deps && tsc -p src/functions/tsconfig.json",
    "build:graphiql": "webpack",
    "build:graphiql-dev": "NODE_ENV=development webpack-dev-server",
    "build:functions-dev": "NODE_ENV=development npm run build:functions -- --watch",
    "test:lint": "tslint --project tsconfig.json",
    "test:types": "tsc --noEmit --skipLibCheck",
    "serve": "firebase emulators:start",
    "dev": "NODE_ENV=development npm run build:functions-deps && concurrently npm:build:functions-dev npm:serve npm:build:graphiql-dev",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-display-name": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@graphql-codegen/add": "^1.12.2",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.1.3",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "apollo-server": "^2.10.0",
    "apollo-server-express": "^2.10.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "firebase": "^7.8.1",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^7.12.1",
    "graphiql": "^0.17.5",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebaseui": "^4.1.0",
    "react-refresh": "^0.7.2",
    "regenerator-runtime": "^0.13.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  }
}
